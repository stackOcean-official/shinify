\name{shinify}
\alias{shinify}
\title{Shiny Server}
\usage{
shinify(ml_model, modeltype = "", title = "")
}
\arguments{
\item{modeltype}{type of your model. Currently for "log_reg" or "lin_reg"}

\item{model}{Your R model}
}
\description{
This function creates a shiny server for your model
}
\examples{
shinify(model)
shinify(model, "log_reg")
shinify(model, "log_reg", "your awesome title")
}
\keyword{shiny}
